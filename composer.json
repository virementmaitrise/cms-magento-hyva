{
  "name": "virementmaitrise/payment-hyva",
  "description": "Virement MaitrisÃ© enables users to pay securely and directly from their bank account.",
  "keywords": [
    "payment",
    "bank"
  ],
  "homepage": "https://github.com/virementmaitrise/cms-magento-hyva",
  "support": {
    "email": "contact@fintecture.com"
  },
  "type": "magento2-module",
  "version": "1.1.1",
  "license": [
    "GPL-3.0"
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-openssl": "*",
    "magento/framework": "^102.0 || ^103.0",
    "magento/module-developer": ">=100",
    "magento/module-payment": ">=100",
    "magento/module-checkout": ">=100",
    "magento/module-sales": ">=100",
    "symfony/http-client": "*",
    "nyholm/psr7": "*",
    "magewirephp/magewire": "^1.13",
    "hyva-themes/magento2-hyva-checkout": "^1.3",
    "virementmaitrise/virementmaitrise-sdk-php": "^2.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^1",
    "bitexpert/phpstan-magento": "^0.30.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/extension-installer": "^1.1",
    "phpunit/phpunit": "*"
  },
  "suggest": {
    "chillerlan/php-qrcode": "QR Code library for payment with \"Login as Customer\" feature enabled"
  },
  "repositories": {
    "private-packagist": {
      "type": "composer",
      "url": "https://hyva-themes.repo.packagist.com/fintecture-com-bhd0m"
    },
    "0": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Virementmaitrise\\Payment\\": "."
    }
  },
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": true,
      "phpstan/extension-installer": true,
      "php-http/discovery": true
    }
  }
}
